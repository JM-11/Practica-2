<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ado.com.orange.jovargas.practica_2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LinearLayoutVerticalActivity" />
        <activity
            android:name=".LinearLayoutHorizontalActivity"
            android:label="@string/title_activity_linear_layout_horizontal"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".RelativeLayout"
            android:label="@string/title_activity_relative_layout"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".LinearLayoutTable" />
        <activity android:name=".FrameLayout" />
        <activity android:name=".ListView"></activity>
    </application>

</manifest>